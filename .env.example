# Environment Variables for Secure Password Manager
# Copy this file to .env and set your actual values

# JWT Secret Key - Must be a cryptographically secure string
# Minimum 32 characters, mixed case, numbers, and symbols
JWT_SECRET=Your-Super-Secure-JWT-Secret-Key-Here-2024!

# Database Configuration
DATABASE_URL=sqlite:///password_manager.db
SQLALCHEMY_DATABASE_URI=sqlite:///password_manager.db

# Optional: Development settings
DEBUG=False
